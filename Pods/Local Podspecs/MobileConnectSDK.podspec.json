{
  "name": "MobileConnectSDK",
  "version": "3.0.2",
  "summary": "MobileConnectSDK is a framework for accessing Mobile Connect services for fast login.",
  "homepage": "https://developer.mobileconnect.io",
  "license": {
    "type": "MIT",
    "file": "Metadata/LICENSE"
  },
  "authors": {
    "Dan Andoni": "dan.andoni@bjss.com"
  },
  "social_media_url": "https://developer.mobileconnect.io",
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source": {
    "git": "https://github.com/Dan-Andoni-BJSS/GMC-235Test",
    "branch": "master",
    "tag": "3.0.2"
  },
  "resources": "MobileConnectSDK/AdditionalFiles/*.{xcassets,storyboard,strings}",
  "source_files": "MobileConnectSDK/*.{h,plist,swift}",
  "dependencies": {
    "JSONModel": [
      "~> 1.2.0"
    ],
    "Alamofire": [
      "~> 3.4"
    ]
  },
  "subspecs": [
    {
      "name": "Extensions",
      "source_files": "MobileConnectSDK/Extensions/*.swift"
    },
    {
      "name": "Services",
      "source_files": "MobileConnectSDK/Services/*.swift",
      "subspecs": [
        {
          "name": "AttributeService",
          "source_files": "MobileConnectSDK/Services/AttributeService/*.swift"
        },
        {
          "name": "BaseMobileConnect",
          "source_files": "MobileConnectSDK/Services/BaseMobileConnect/*.swift"
        },
        {
          "name": "MobileConnect",
          "source_files": "MobileConnectSDK/Services/MobileConnect/*.swift",
          "subspecs": [
            {
              "name": "Configurations",
              "source_files": "MobileConnectSDK/Services/MobileConnect/Configurations/*.swift"
            }
          ]
        },
        {
          "name": "Discovery",
          "source_files": "MobileConnectSDK/Services/Discovery/*.swift"
        }
      ]
    },
    {
      "name": "Models",
      "source_files": "MobileConnectSDK/Models/*.{h,m,swift}",
      "subspecs": [
        {
          "name": "Enums",
          "source_files": "MobileConnectSDK/Models/Enums/*.*"
        }
      ]
    },
    {
      "name": "MobileConnectManager",
      "source_files": "MobileConnectSDK/MobileConnectManager/*.swift"
    },
    {
      "name": "Controllers",
      "source_files": "MobileConnectSDK/Controllers/*.swift"
    },
    {
      "name": "Views",
      "source_files": "MobileConnectSDK/Views/*.swift"
    },
    {
      "name": "Utilities",
      "source_files": "MobileConnectSDK/Utilities/*.swift",
      "subspecs": [
        {
          "name": "Networking",
          "source_files": "MobileConnectSDK/Utilities/Networking/*.swift",
          "subspecs": [
            {
              "name": "RequestConstructor",
              "source_files": "MobileConnectSDK/Utilities/Networking/RequestConstructor/*.swift"
            }
          ]
        }
      ]
    }
  ]
}
